# TailorCloud 開発環境設定ファイル
# このファイルをコピーして development.env を作成し、実際の値を設定してください

# 環境タイプ
ENV=development

# APIベースURL（開発環境のAPIサーバー）
API_BASE_URL=http://localhost:8080

# Firebase設定（使用する場合）
ENABLE_FIREBASE=false
FIREBASE_API_KEY=
FIREBASE_APP_ID=
FIREBASE_PROJECT_ID=
FIREBASE_MESSAGING_SENDER_ID=

# デフォルトテナントID
DEFAULT_TENANT_ID=tenant-123

